{
    "$schema": "https://raw.githubusercontent.com/DanielGavin/ols/master/misc/ols.schema.json",
    "profiles": [{ "name": "default", "checker_path": ["src"] }],
    "profile": "default",
    "checker_args": ["-vet", "-warnings-as-errors"],
    "enable_format": false,
    "enable_hover": true,
    "enable_document_symbols": true,
    "enable_fake_methods": false,
    "enable_references": true,
    "enable_document_highlights": true,
    "enable_document_links": false,
    "enable_completion_matching": true,
    "enable_inlay_hints_params": false,
    "enable_inlay_hints_default_params": false,
    "enable_inlay_hints_implicit_return": false,
    "enable_semantic_tokens": true,
    "enable_snippets": false,
    "enable_procedure_snippet": false,
    "enable_checker_only_saved": false,
    "enable_auto_import": true
}
